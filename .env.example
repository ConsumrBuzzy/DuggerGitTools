# DuggerGitTools Environment Variables
# Copy this file to .env and fill in your actual values

# Git Configuration (optional - will use system git config if not set)
GIT_NAME=
GIT_EMAIL=

# API Keys (add as needed for integrations)
# GITHUB_TOKEN=
# GITLAB_TOKEN=
# BITBUCKET_TOKEN=

# Logging Configuration
LOG_LEVEL=INFO

# DGT Configuration
DGT_AUTO_PUSH=true
DGT_DRY_RUN=false